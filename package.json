{
  "name": "nft",
  "version": "0.0.0",
  "private": true,
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"npm run electron:dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npm run build && npm run electron:start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "electron:start": "electron-forge start",
    "electron:dev": "cross-env IS_DEV=true PORT='5173' electron-forge start",
    "test":"concurrently -k \"vite build && vite preview\"  \"cross-env IS_DEV=true PORT='4173' electron-forge start\" "
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^25.1.1",
    "vite": "^4.3.9"
  }
}
